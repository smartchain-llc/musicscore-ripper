{
  "description":"Download sheet music from musicscore fetches",
  "manifest_version" : 2,
  "name" : "MusicScore Ripper",
  "version": "1.0",
  "permissions":[
      "webRequest",
      "<all_urls>"
  ],
  "content_scripts" : [
    { "matches" : ["*://musescore.com/user/*/scores/*"], "js": ["./build/main.js"]}
  ]
}